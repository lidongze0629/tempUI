  <script>
    var if_waiting = "<%=if_waiting%>"
    if (if_waiting == 1) {
      alert("There is a Job in queue , Waiting....")
    }

    function before_submit() {
      var algorithm = document.getElementById("algo").value
      var Reg = /^\d+$/
      var size = 81306
      var begin = 1
      var cu_dataset = $("#dataset option:selected")
      if (cu_dataset.text() == "Twitter") { //begin 1
        size = 81306
        begin = 1
      } else if (cu_dataset.text() == "LiveJournal") { //begin 1
        size = 4847571
        begin = 1
      } else if (cu_dataset.text() == "com-friendster") { //begin 0
        size = 65608365
        begin = 0
      }
      if (algorithm == "sssp") {
        var query_value = document.getElementById("exampleInputPassword1").value
        if (query_value == "") {
          alert("Input is Null , Please input you query");
          $("#exampleInputPassword1").focus()
          return false;
        } else if (query_value.match(Reg) == null) {
          alert("Input Format Error , Please Input number")
          $("#exampleInputPassword1").focus()
          return false;
        } else if ((query_value < begin) || (query_value > dataset_size)) {
          alert("Input out of bound , See Right Console")
          $("#exampleInputPassword1").focus()
          return false;
        }
      }
    }
  </script>
