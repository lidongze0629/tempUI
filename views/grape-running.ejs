  <script>
    function kill_worker() {
      $.post('/kill_worker', function(data) {});
    }
    $('#sim-modal').on('hidden.bs.modal', function(e) {
      $("#selected_pattern").show();
    })

    $(".sim-select").click(function() {
      console.log("clicked:" + $(this).attr("name"));
      $("#pattern_name").attr("value", $(this).attr("name"));
      $pattern_source = "/images/sim-pattern/" + $(this).attr("name") + ".jpg";
      $("#selected_pattern img").attr("src", $pattern_source)
    });


    $("#algo").bind("change", function() {
      if ($(this).val() == "cc") {
        $('#QueryPattern').toggle(false);
        $('#exampleInputPassword1').toggle(false);
      } else
      if ($(this).val() == "sssp") {
        $('#QueryPattern').toggle(false);
        $('#exampleInputPassword1').toggle(true);
      } else
      if ($(this).val() == "sim") {
        $('#QueryPattern').toggle(true);
        $('#exampleInputPassword1').toggle(false);
        $("#sim-pattern-btn").show();
      }
    });


    var algo = $("#algo option:selected");
    var dataset = $("#dataset option:selected")
    var workerNum = $("#workerNum option:selected")
    $("#console_content").html("Current Choice : <br /> Algorithm : " + algo.text() + "<br /> dataset : " + dataset.text() + "<br />  worker Number : " + workerNum.text())
    $("#console_content").append("<br /> Input : Null");
    var dataset_size = 81306 //default for twitter
    var begin = 1

    $("#algo").change(function() {
      document.getElementById("exampleInputPassword1").value = ""
      algo = $("#algo option:selected");
      $("#console_content").html("Current Choice : <br /> Algorithm : " + algo.text() + "<br /> dataset : " + dataset.text() + "<br />  worker Number : " + workerNum.text())
      $("#console_content").append("<br /> Input : Null");
    });
    $("#dataset").change(function() {
      document.getElementById("exampleInputPassword1").value = ""
      dataset = $("#dataset option:selected")
      $("#console_content").html("Current Choice : <br /> Algorithm : " + algo.text() + "<br /> dataset : " + dataset.text() + "<br />  worker Number : " + workerNum.text())
      $("#console_content").append("<br /> Input : Null");
      if (dataset.text() == "Twitter") { //begin 1
        dataset_size = 81306
        begin = 1
      } else if (dataset.text() == "LiveJournal") { //begin 1
        dataset_size = 4847571
        begin = 1
      } else if (dataset.text() == "com-friendster") { //begin 0
        dataset_size = 65608365
        begin = 0
      }
    });
    $("#workerNum").change(function() {
      var query_content = document.getElementById("exampleInputPassword1").value;
      workerNum = $("#workerNum option:selected")
      $("#console_content").html("Current Choice : <br /> Algorithm : " + algo.text() + "<br /> dataset : " + dataset.text() + "<br />  worker Number : " + workerNum.text())
      $("#console_content").append("<br /> Input : " + query_content);
    });
    $("#exampleInputPassword1").change(function() {
      var query_content = document.getElementById("exampleInputPassword1").value;
      var Reg = /^\d+$/
      if (query_content.match(Reg) == null) {
        $("#console_content").html("Current Choice : <br /> Algorithm : " + algo.text() + "<br /> dataset : " + dataset.text() + "<br />  worker Number : " + workerNum.text())
        $("#console_content").append("<br /> Input : Currently Query Input : Error input format ")
      } else {
        if ((query_content >= begin) && (query_content <= dataset_size)) {
          $("#console_content").html("Current Choice : <br /> Algorithm : " + algo.text() + "<br /> dataset : " + dataset.text() + "<br />  worker Number : " + workerNum.text())
          $("#console_content").append("<br /> Input : " + query_content);
        } else {
          $("#console_content").html("Current Choice : <br /> Algorithm : " + algo.text() + "<br /> dataset : " + dataset.text() + "<br />  worker Number : " + workerNum.text())
          $("#console_content").append("<br/> The Source you choice is out of bound , you should choice <br> [1-81306] for Twitter <br> [1-4847571] for LiveJournal<br> [0-65608365] for com-friendster")
        }
      }
    });
  </script>
